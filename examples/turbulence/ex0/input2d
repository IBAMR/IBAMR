// computational domain
HEIGHT = 2.0
LENGTH = 2.0

RHO = 1.0
MU  = 0.0017
WALL_LOCATION_INDEX = 2, 3
DISTANCE_TO_VIRTUAL_POINT = 2.0

// specifying the exact pressure gradient
USourceFunction{
    AnalyticalPressureGradient = 1.0
}

// grid spacing parameters
L          = 1.0
MAX_LEVELS = 1       // maximum number of levels in locally refined grid
REF_RATIO  = 2        // refinement ratio between levels
REF_RATIO_FINEST = 2  // refinement ratio for the finest level.

Ny = 25                                 // coarsest grid spacing
Nx = Ny
H_COARSEST = Ny                          // coarsest grid spacing
NFINEST = (REF_RATIO^(MAX_LEVELS - 1))*Ny  // finest   grid spacing
HFINEST = LENGTH/NFINEST

// writing the velocity profile across x near the outlet
output_velocity_profile{
number_of_locations = 4
lower_coordinates_0 = 0.5, 0.0
upper_coordinates_0 = 0.5, 2.0
lower_coordinates_1 = 1.0, 0.0
