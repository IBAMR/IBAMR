Main {
   solver_type = SOLVER_TYPE
   adv_diff_solver_type = ADV_DIFF_SOLVER_TYPE
   discretization_form  = DISCRETIZATION_FORM

// log file parameters
   log_file_name               = "turbulent_channel_flow.log"
   log_all_nodes               = FALSE

// visualization dump parameters
   viz_writer                  = "VisIt"
   viz_dump_interval           = 1000
   viz_dump_dirname            = "turbulent_channel_flow"
   visit_number_procs_per_file = 1

// restart dump parameters
   restart_dump_interval       = 0
   restart_dump_dirname        = "restart_INS2d"
   
// post-processing data output parameters
    data_dump_interval         = 10
    data_dump_dirname          = "velocity_profile"

// timer dump parameters
   timer_dump_interval         = 0
}

CartesianGeometry {
   domain_boxes = [ (0,0) , ( Nx - 1, Ny - 1) ]
   x_lo         =   0.0,   0.0           // lower end of computational domain.
   x_up         =   LENGTH,   HEIGHT         // upper end of computational domain.
   periodic_dimension = 1, 0
}

GriddingAlgorithm {
   max_levels = MAX_LEVELS           // Maximum number of levels in hierarchy.
   ratio_to_coarser {
      level_1 = REF_RATIO,REF_RATIO  // vector ratio to next coarser level
      level_2 = REF_RATIO,REF_RATIO
      level_3 = REF_RATIO,REF_RATIO
      level_4 = REF_RATIO,REF_RATIO
      level_5 = REF_RATIO,REF_RATIO
      level_6 = REF_RATIO,REF_RATIO
      level_7 = REF_RATIO,REF_RATIO
   }

   largest_patch_size {
      level_0 = 2048, 2048 // largest patch allowed in hierarchy
                          // all finer levels will use same values as level_0...
   }

   smallest_patch_size {
      level_0 =  8, 8 // smallest patch allowed in hierarchy
                            // all finer levels will use same values as level_0...
   }

   allow_patches_smaller_than_minimum_size_to_prevent_overlaps = TRUE
   efficiency_tolerance   = 0.80e0    // min % of tag cells in new patch level
   combine_efficiency     = 0.80e0    // chop box if sum of volumes of smaller
                                      // boxes < efficiency * vol of large box
}

x_lower_index_0 = 0
y_lower_index_0 = int((Ny/8) - 1)
x_upper_index_0 = int(10*Nx - 1)
y_upper_index_0 = int((Ny/4) - 1)

x_lower_index_1 = 0
y_lower_index_1 = int((3*Ny/4) - 1)
x_upper_index_1 = int(10*Nx - 1)
y_upper_index_1 = int((7*Ny/8) - 1)

x_lower_index_2 = 0 // int(0.0/H_COARSEST)
y_lower_index_2 = 0 // int(0.0/H_COARSEST)
x_upper_index_2 = int (10*Nx - 1) // int(LENGTH/H_COARSEST)
y_upper_index_2 = int((Ny/8) - 1) // int((HEIGHT/8)/H_COARSEST)

x_lower_index_3 = 0
y_lower_index_3 = int((7*Ny/8) - 1)
x_upper_index_3 = int(10*Nx - 1)
y_upper_index_3 = int(Ny - 1)

StandardTagAndInitialize {
   tagging_method = "REFINE_BOXES"
   RefineBoxes {
      Level0{
        boxes_0 = [ ( x_lower_index_0 , y_lower_index_0 ) , ( x_upper_index_0 , y_upper_index_0 ) ]
        boxes_1 = [ ( x_lower_index_1 , y_lower_index_1 ) , ( x_upper_index_1 , y_upper_index_1 ) ]
        }
        Level1{
        boxes_0 = [ ( x_lower_index_2 , y_lower_index_2 ) , ( x_upper_index_2 , y_upper_index_2 ) ]
        boxes_1 = [ ( x_lower_index_3 , y_lower_index_3 ) , ( x_upper_index_3 , y_upper_index_3 ) ]
        }
      }
}

LoadBalancer {
   bin_pack_method     = "SPATIAL"
   max_workload_factor = 1
}

TimerManager{
   print_exclusive = FALSE
   print_total = TRUE
   print_threshold = 0.1

   timer_list = "IBAMR::*::*" , "IBTK::*::*" , "*::*::*"
}
