// application parameters
X_UR  = 0.05715,0.05715,0.05715
L     = 0.4
H     = 0.1
B     = 0.05715
GRAV_CONST = 0.0,0.0,-9.81


// material properties
MU_I       = 1.137e-3
MU_O       = 1.78e-5
RHO_I      = 1.0e3
RHO_O      = 1.226

NUM_INTERFACE_CELLS = 1.0

// grid spacing parameters
MAX_LEVELS = 2        // maximum number of levels in locally refined grid
REF_RATIO  = 2        // refinement ratio between levels

Nz = 32
Ny = 18
Nx = 4*Nz                                   // coarsest grid spacing
NXFINEST = (REF_RATIO^(MAX_LEVELS - 1))*Nx  // finest   grid spacing
NYFINEST = (REF_RATIO^(MAX_LEVELS - 1))*Ny  // finest   grid spacing
NZFINEST = (REF_RATIO^(MAX_LEVELS - 1))*Nz


// Level set parameters
LS_ORDER           = "THIRD_ORDER_ENO"
LS_ABS_TOL         = 1e-8
LS_REINIT_INTERVAL = 1
MAX_ITERATIONS     = NYFINEST/4
APPLY_SIGN_FIX     = TRUE
APPLY_SUBCELL_FIX  = TRUE
APPLY_VOLUME_SHIFT = FALSE
APPLY_MASS_CONSTRAINT = FALSE

LS_TAG_VALUE = 0.0
LS_TAG_ABS_THRESH = 2.0*L/Nx
VORTICITY_REL_THRESH = 0.5

// surface tension parameters
DELTA_FUNCTION = "IB_4"
SIGMA = 0.0728

U = "0.0"
V = "0.0"
W = "0.0"


