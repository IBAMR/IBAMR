// physical parameters
Re = 100
RHO   = 1.0                                         // mass density of fluid (gm/cm^3)
MU    = 0.01                                        // dynamic viscosity of fluid (gm/(cm s))
U_MAX = 1.0                                      // approximate maximum velocity in the entire domain (cm/2)

PI = 3.14159265359
THETA = 0.0


// discretization parameters
MAX_LEVELS = 1                                      // maximum number of levels in locally refined grid
REF_RATIO  = 1                                      // refinement ratio between levels
N =64                                 // actual    number of grid cells on coarsest grid level
NFINEST = (REF_RATIO^(MAX_LEVELS - 1))*N            // effective number of grid cells on finest   grid level
L = 5.0                                           // channel length (cm)
H = 5.0                                           // domain  height (cm)
D = 1.0
                                          // body    width  (cm)
P_E   = 8* MU * L*U_MAX/(D*D)                                         // external pressure (dyne/cm^2)
DX0 = H/N                                           // mesh width on coarsest grid level (cm)
DX  = H/NFINEST                                     // mesh width on finest   grid level (cm)
MFAC = 2.0                                    // ratio of Lagrangian mesh width to Cartesian mesh width for thin structure
ELEM_TYPE = "QUAD4"                                 // type of element to use for thin structure discretization
elem_order_thin = "FIRST"                                // finite element order

//Computing velocity profile
X_LOC_MIN = 0.1*L
X_LOC_MAX = 0.9*L
Z_LOC_MIN = 0.0
Z_LOC_MAX = H
Y_LOC_MIN = 0.0 
Y_LOC_MAX = H 

Yo = H/2
Zo = H/2
Yi = H/2
Zi = H/2

FEEDBACK_FORCER = FALSE

// solver parameters
IB_DELTA_FUNCTION          = "IB_3"            // the type of smoothed delta function to use for Lagrangian-Eulerian interaction
SPLIT_NORMAL_FORCES        = TRUE           // whether to split interior and boundary forces
SPLIT_TANGENTIAL_FORCES	   = TRUE
USE_PRESSURE_JUMP_CONDITIONS        = TRUE           // whether to impose pressure jumps at fluid-structure interfaces
USE_VELOCITY_JUMP_CONDITIONS        = TRUE         // whether to impose pressure jumps at fluid-structure interfaces
WSS_CALC_WIDTH 				= 1.05
P_CALC_WIDTH 			   = 1.5
USE_CONSISTENT_MASS_MATRIX = TRUE              // whether to use a consistent or lumped mass matrix
IB_POINT_DENSITY           = 1                    // approximate density of IB quadrature points for Lagrangian-Eulerian interaction
SOLVER_TYPE                = "STAGGERED"            // the fluid solver to use (STAGGERED or COLLOCATED)
CFL_MAX                    = 0.10                   // maximum CFL number
DT       				   = 0.5*CFL_MAX*DX        // maximum timestep size
START_TIME                 = 0.0e0                  // initial simulation time
END_TIME                   = 10 * DT            // final simulation time
GROW_DT                    = 2.0e0                  // growth factor for timesteps
NUM_CYCLES                 = 1                      // number of cycles of fixed-point iteration
//VISCOUS_TS_TYPE = "BACKWARD_EULER"
CONVECTIVE_TS_TYPE         = "ADAMS_BASHFORTH"      // convective time stepping type
CONVECTIVE_OP_TYPE         = "STABILIZED_PPM"       // convective differencing discretization type
CONVECTIVE_FORM            = "ADVECTIVE"            // how to compute the convective terms
NORMALIZE_PRESSURE         = FALSE                   // whether to explicitly force the pressure to have mean zero
ERROR_ON_DT_CHANGE         = TRUE                   // whether to emit an error message if the time step size changes
VORTICITY_TAGGING          = TRUE                   // whether to tag cells for refinement based on vorticity thresholds
TAG_BUFFER                 = 1                      // size of tag buffer used by grid generation algorithm
REGRID_CFL_INTERVAL        = 0.5                    // regrid whenever any material point could have moved 0.5 meshwidths since previous regrid
OUTPUT_U                   = TRUE
OUTPUT_P                   = TRUE
OUTPUT_F                   = TRUE
OUTPUT_OMEGA               = TRUE
OUTPUT_DIV_U               = TRUE
ENABLE_LOGGING             = TRUE


// Penalty parameter
KAPPA_S_THIN = 0.002/DT^2 //5000 //0.0025/DT^2 //0.018*DX/DT^2 //1000 //4.0*DX/(DT^2) 
ETA_S_THIN = 0.0 //4.0e-2/DT 


// Parabolic profile
U = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) >= D/2) ? 0.0 : U_MAX*(1.0 - 4.0*((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H))/(D*D))"
V = "0.0"
W = "0.0"
FFAC = P_E*D/(L)    //slope of inner profile for the linear form of the velocity
P = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) > D/2) ? 0.0 : -2.*p_e*(X0)/(L) + p_e"



X1_lower = 0.5*H - 0.5*D/cos(THETA) - 0.5*L*tan(THETA) 
X1_upper = X1_lower + D/cos(THETA)


VelocityInitialConditions {

	p_e = P_E
    theta = THETA
    D = D
    w = W
    H = H
    L = L
    X1_lower = X1_lower 
    X1_upper = X1_upper 
    mu = MU
   U_MAX = U_MAX
   function_0 = U
   function_1 = V
   function_2 = W
}


PressureInitialConditions {
    D = D
    L = L
    H = H
    U_MAX = U_MAX
    mu = MU
    p_e = P_E
    theta = THETA
    X1_lower = X1_lower 
    X1_upper = X1_upper 

    function = P
}


VelocityBcCoefs_0 {
    p_e = P_E
    D = D
    L = L
    X1_lower = 0.5*H - 0.5*D 
    X1_upper = X1_lower + D
    mu = MU
    H = H
    U_MAX = U_MAX


    acoef_function_0 = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) < D/2) ?  0.0 : 1.0"
    acoef_function_1 = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) < D/2) ? 0.0 : 1.0"
    acoef_function_2 = "1.0"
    acoef_function_3 = "1.0"
    acoef_function_4 = "1.0"
    acoef_function_5 = "1.0"

    bcoef_function_0 = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) < D/2) ? 1.0 : 0.0"
    bcoef_function_1 = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) < D/2) ? 1.0 : 0.0"
    bcoef_function_2 = "0.0"
    bcoef_function_3 = "0.0"
    bcoef_function_4 = "0.0"
    bcoef_function_5 = "0.0"

    gcoef_function_0 = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) < D/2) ? -p_e : 0.0" 
    gcoef_function_1 = "(sqrt((X1 - 0.5*H)*(X1 - 0.5*H) + (X2 - 0.5*H)*(X2 - 0.5*H)) < D/2) ? p_e : 0.0" 
    gcoef_function_2 = "0.0"
    gcoef_function_3 = "0.0"
    gcoef_function_4 = "0.0"
    gcoef_function_5 = "0.0"
}


VelocityBcCoefs_1 {

    p_e = P_E
    D = D
    L = L
    X1_lower = X1_lower 
    X1_upper = X1_upper
    mu = MU
    H = H

   acoef_function_0 = "1.0"
   acoef_function_1 = "1.0"
   acoef_function_2 = "0.0"
   acoef_function_3 = "0.0"
   acoef_function_4 = "1.0"
   acoef_function_5 = "1.0"
   
   bcoef_function_0 = "0.0"
   bcoef_function_1 = "0.0"
   bcoef_function_2 = "1.0"
   bcoef_function_3 = "1.0"
   bcoef_function_4 = "0.0"
   bcoef_function_5 = "0.0"

   gcoef_function_0 = "0.0"
   gcoef_function_1 = "0.0"
   gcoef_function_2 = "0.0"
   gcoef_function_3 = "0.0"
   gcoef_function_4 = "0.0"
   gcoef_function_5 = "0.0"
}


VelocityBcCoefs_2 {


   acoef_function_0 = "1.0"
   acoef_function_1 = "1.0"
   acoef_function_2 = "1.0"
   acoef_function_3 = "1.0"
   acoef_function_4 = "0.0"
   acoef_function_5 = "0.0"
   
   bcoef_function_0 = "0.0"
   bcoef_function_1 = "0.0"
   bcoef_function_2 = "0.0"
   bcoef_function_3 = "0.0"
   bcoef_function_4 = "1.0"
   bcoef_function_5 = "1.0"

   gcoef_function_0 = "0.0"
   gcoef_function_1 = "0.0"
   gcoef_function_2 = "0.0"
   gcoef_function_3 = "0.0"
   gcoef_function_4 = "0.0"
   gcoef_function_5 = "0.0"
}

